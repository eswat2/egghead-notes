{
  "name": "egghead-notes",
  "version": "0.0.2",
  "private": true,
  "devDependencies": {
    "babel-core": "6.26.3",
    "husky": "4.2.1",
    "lint-staged": "10.0.3",
    "react-scripts": "3.3.0"
  },
  "dependencies": {
    "axios": "0.19.2",
    "express": "4.17.1",
    "express-history-api-fallback": "2.2.1",
    "express-sslify": "1.2.0",
    "lodash": "^4.17.15",
    "lodash.template": "^4.5.0",
    "mobx": "5.15.4",
    "mobx-react": "6.1.5",
    "mobx-react-devtools": "6.1.1",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "fix": "eslint app.js ./src --ext .js --fix",
    "lintr": "esw app.js ./src --ext .js --color",
    "watch": "npm run lintr -- --watch",
    "report": "eslint app.js ./src --ext .js -f html > eslint.html",
    "deploy": "git push heroku master",
    "open": "open https://egghead-notes.herokuapp.com"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
