{
  "name": "egghead-notes",
  "version": "0.0.3",
  "private": true,
  "devDependencies": {
    "babel-core": "6.26.3",
    "react-scripts": "4.0.3"
  },
  "dependencies": {
    "axios": "0.21.1",
    "express": "4.17.1",
    "express-history-api-fallback": "2.2.1",
    "express-sslify": "1.2.0",
    "lodash": "4.17.21",
    "lodash.template": "^4.5.0",
    "mobx": "6.1.7",
    "mobx-react": "7.1.0",
    "mobx-react-devtools": "6.1.1",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
